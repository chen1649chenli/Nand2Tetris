class Point {
    field int x, y;
    static int pointCount;

    constructor Point new(int ax, int ay) {
        let x = ax;
        let y = ay;
        let pointCount = pointCount + 1;
        return this;
    }

    method int getx() {
        return x;
    }

    method int gety() {
        return y;
    }


    method void print() {
        do Output.printInt(x);
        do Output.printInt(y);
        return;
    }
}